{
  "environment": "production",
  "version": "1.0.0",
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "workers": 4,
    "reload": false,
    "log_level": "info"
  },
  "database": {
    "type": "postgresql",
    "host": "${DB_HOST}",
    "port": 5432,
    "name": "roboken_prod",
    "user": "${DB_USER}",
    "password": "${DB_PASSWORD}",
    "pool_size": 20,
    "max_overflow": 10,
    "pool_recycle": 3600
  },
  "redis": {
    "host": "${REDIS_HOST}",
    "port": 6379,
    "db": 0,
    "password": "${REDIS_PASSWORD}"
  },
  "security": {
    "jwt_secret": "${JWT_SECRET}",
    "jwt_algorithm": "HS256",
    "access_token_expire_minutes": 30,
    "refresh_token_expire_days": 7,
    "password_min_length": 8,
    "require_email_verification": true,
    "max_login_attempts": 5,
    "lockout_duration_minutes": 15
  },
  "api_keys": {
    "openai": "${OPENAI_API_KEY}",
    "anthropic": "${ANTHROPIC_API_KEY}",
    "stripe": "${STRIPE_API_KEY}",
    "google_maps": "${GOOGLE_MAPS_API_KEY}",
    "serpapi": "${SERPAPI_API_KEY}",
    "n8n_cloud": "${N8N_CLOUD_API_KEY}"
  },
  "stripe": {
    "publishable_key": "${STRIPE_PUBLISHABLE_KEY}",
    "secret_key": "${STRIPE_SECRET_KEY}",
    "webhook_secret": "${STRIPE_WEBHOOK_SECRET}",
    "currency": "usd"
  },
  "pricing": {
    "free_plan": {
      "name": "Free",
      "price_monthly": 0,
      "max_tasks_per_month": 100,
      "max_tokens_per_task": 1000,
      "support_level": "community"
    },
    "standard_plan": {
      "name": "Standard",
      "price_monthly": 30,
      "price_yearly": 300,
      "max_tasks_per_month": 4800,
      "max_tokens_per_task": 10000,
      "support_level": "email",
      "gross_margin_target": 0.60,
      "affiliate_commission": 0.20
    },
    "premium_plan": {
      "name": "Premium",
      "price_monthly": 200,
      "price_yearly": 2000,
      "max_tasks_per_month": 32000,
      "max_tokens_per_task": 50000,
      "support_level": "priority",
      "gross_margin_target": 0.60,
      "affiliate_commission": 0.20
    }
  },
  "ai_models": {
    "nlp_model": "bert-base-uncased",
    "vision_model": "yolov5",
    "meta_learning_model": "maml",
    "default_llm": "gpt-4",
    "fallback_llm": "claude-3-sonnet",
    "max_tokens": 4096,
    "temperature": 0.7
  },
  "automation": {
    "max_concurrent_tasks": 10,
    "task_timeout_seconds": 300,
    "retry_attempts": 3,
    "retry_delay_seconds": 5,
    "enable_human_approval": true,
    "approval_timeout_minutes": 60
  },
  "n8n": {
    "api_url": "${N8N_API_URL}",
    "api_key": "${N8N_API_KEY}",
    "webhook_url": "${N8N_WEBHOOK_URL}",
    "max_workflow_execution_time": 600
  },
  "aws": {
    "region": "us-east-1",
    "s3_bucket": "roboken-prod-storage",
    "cloudwatch_log_group": "/aws/roboken/prod",
    "secrets_manager_prefix": "roboken/prod/"
  },
  "monitoring": {
    "enable_prometheus": true,
    "prometheus_port": 9090,
    "enable_sentry": true,
    "sentry_dsn": "${SENTRY_DSN}",
    "log_level": "INFO",
    "enable_performance_tracking": true
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_minute": 60,
    "requests_per_hour": 1000,
    "requests_per_day": 10000
  },
  "cors": {
    "allowed_origins": [
      "https://roboken.ai",
      "https://www.roboken.ai"
    ],
    "allowed_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allowed_headers": ["*"],
    "allow_credentials": true
  },
  "email": {
    "smtp_host": "${SMTP_HOST}",
    "smtp_port": 587,
    "smtp_user": "${SMTP_USER}",
    "smtp_password": "${SMTP_PASSWORD}",
    "from_email": "noreply@roboken.ai",
    "from_name": "RoboKen"
  },
  "features": {
    "enable_web_automation": true,
    "enable_mobile_automation": true,
    "enable_desktop_automation": true,
    "enable_workflow_automation": true,
    "enable_api_access": true,
    "enable_custom_integrations": false,
    "enable_team_collaboration": true
  }
}
